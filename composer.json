{
    "name": "clarkwinkelmann/flarum-ext-gate-logger",
    "description": "Log all gate queries for debug purpose",
    "keywords": [
        "flarum",
        "debug",
        "gate"
    ],
    "type": "flarum-extension",
    "license": "MIT",
    "require": {
        "flarum/core": "^1.0"
    },
    "authors": [
        {
            "name": "Clark Winkelmann",
            "email": "clark.winkelmann@gmail.com",
            "homepage": "https://clarkwinkelmann.com/",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/clarkwinkelmann/flarum-ext-gate-logger/issues",
        "source": "https://github.com/clarkwinkelmann/flarum-ext-gate-logger",
        "forum": "https://discuss.flarum.org/d/29351"
    },
    "autoload": {
        "psr-4": {
            "ClarkWinkelmann\\GateLogger\\": "src/"
        }
    },
    "extra": {
        "flarum-extension": {
            "title": "Gate Logger",
            "category": "feature",
            "icon": {
                "name": "fas fa-bug",
                "backgroundColor": "#dedede",
                "color": "#333"
            }
        }
    }
